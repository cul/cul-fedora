- members_list = get_members(document)
- if document["object_display"]
 %h3 Member Items
 %div#imageMembers
  - rows = get_rows(members_list,3)
  - rows.each do |row|
   %div{:class=>["yui-gb", (row==rows.first)?"first":false]}
    - row.each do |j|
     %div{:class=>["yui-u", (j==row.first)?"first":false]}
      - if !j.nil?
       - member = members_list[j]
       %p=link_to_document(member)
       - if member["format"] == "image"
        = render_document_partial(members_list[j], :thumbnail)
