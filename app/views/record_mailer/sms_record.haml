- @documents.each do |document|
  - if document.respond_to?(:to_marc) and ! document.to_marc['245'].nil?
    = document.to_marc['245']['a'] unless document.to_marc['245']['a'].nil?
  - if document.respond_to?(:to_marc) and ! document.to_marc['100'].nil?
    = " by #{document.to_marc['100']['a']}" unless document.to_marc['100']['a'].nil?
="Link: #{ catalog_path(document[:id], :only_path => false, :host => @host)}"
