- title "Report"

%p
  %strong Name:
  =h @report.name
%p
  %strong Category:
  =h @report.category
%p
  %strong Generated On:
  =h @report.generated_on
%p
  %strong User:
  =h @report.user_id
%p
  %strong Options:
  =h @report.options
%p
  %strong Data:
  =h @report.data

%p
  = link_to "Edit", edit_report_path(@report)
  |
  = link_to "Destroy", @report, :confirm => 'Are you sure?', :method => :delete
  |
  = link_to "View All", reports_path
